<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:vm="clr-namespace:BadEcho.Omnified.Vision.Statistics.ViewModels"
                    xmlns:v="clr-namespace:BadEcho.Omnified.Vision.Statistics.Views"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <DataTemplate DataType="{x:Type vm:FractionalStatisticViewModel}">
        <v:FractionalStatisticView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:WholeStatisticViewModel}">
        <v:WholeStatisticView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:CoordinateStatisticViewModel}">
        <v:CoordinateStatisticView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:StatisticsViewModel}">
        <v:StatisticsView/>
    </DataTemplate>
    
</ResourceDictionary>